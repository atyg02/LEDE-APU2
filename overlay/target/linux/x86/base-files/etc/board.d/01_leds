#!/bin/sh
#
# Copyright (C) 2016 OpenWrt.org
#

. /lib/functions/uci-defaults.sh
. /lib/x86.sh

board_config_update

board=$(x86_board_name)

case "$board" in
pc-engines-apu2)
	ucidef_set_led_netdev "wan" "WAN" "apu2:green:led3" "eth0"
	ucidef_set_led_netdev "lan" "LAN" "apu2:green:led2" "eth1"
	;;
esac

board_config_flush

exit 0
